do local comandos={"rocket","ragdoll","jumpscare","morph","jail","tiny","nightvision","balloon","inverse"};local Players=game:GetService("Players");local UserInputService=game:GetService("UserInputService");local TextChatService=game:GetService("TextChatService");local RunService=game:GetService("RunService");local LocalPlayer=Players.LocalPlayer;local ESP_COLOR=Color3.fromRGB(0,255,0);local gui=Instance.new("ScreenGui",LocalPlayer.PlayerGui);gui.Name="PainelAdmin";gui.ResetOnSpawn=false;local frame=Instance.new("Frame",gui);frame.Size=UDim2.new(0,250,0,340);frame.Position=UDim2.new(0,30,0.5, -170);frame.BackgroundColor3=Color3.fromRGB(0,0,0);frame.BorderSizePixel=0;frame.Active=true;Instance.new("UICorner",frame).CornerRadius=UDim.new(0,12);local glow=Instance.new("UIStroke",frame);glow.Thickness=4;glow.Color=Color3.fromRGB(0,255,255);glow.Transparency=0.2;task.spawn(function() while frame and frame.Parent  do for i=0,0.7,0.05 do task.wait(0.03);if  not (frame and frame.Parent) then return;end glow.Transparency=i;end for i=0.7,0, -0.05 do task.wait(0.03);if  not (frame and frame.Parent) then return;end glow.Transparency=i;end end end);local title=Instance.new("TextLabel",frame);title.Size=UDim2.new(1,0,0,45);title.BackgroundColor3=Color3.fromRGB(20,20,20);title.Text="Vands gostozo ðŸ‘„ðŸ¥’";title.TextColor3=Color3.fromRGB(0,255,255);title.Font=Enum.Font.GothamBlack;title.TextSize=20;Instance.new("UICorner",title).CornerRadius=UDim.new(0,12);local scroll=Instance.new("ScrollingFrame",frame);scroll.Size=UDim2.new(1, -20,1, -120);scroll.Position=UDim2.new(0,10,0,60);scroll.BackgroundTransparency=1;scroll.ScrollBarThickness=4;local layout=Instance.new("UIListLayout",scroll);layout.Padding=UDim.new(0,6);layout.SortOrder=Enum.SortOrder.LayoutOrder;local espFolder=Instance.new("Folder",game.CoreGui);espFolder.Name="ESP_Folder";local function aplicarESP(player) if ( not player or (player==LocalPlayer)) then return;end pcall(function() local character=player.Character;if  not character then return;end local oldEsp=espFolder:FindFirstChild(player.Name   .. "_ESP" );if oldEsp then oldEsp:Destroy();end local hrp=character:FindFirstChild("HumanoidRootPart") or character:FindFirstChild("Torso") or character:FindFirstChild("UpperTorso") ;if  not hrp then return;end if character:FindFirstChild("ESP_Highlight") then character.ESP_Highlight:Destroy();end local hl=Instance.new("Highlight");hl.Name="ESP_Highlight";hl.Adornee=character;hl.DepthMode=Enum.HighlightDepthMode.AlwaysOnTop;hl.FillColor=Color3.fromRGB(255,50,0);hl.FillTransparency=0.3;hl.OutlineColor=Color3.fromRGB(255,0,0);hl.OutlineTransparency=0;hl.Parent=character;local boxAdorn=Instance.new("BoxHandleAdornment");boxAdorn.Name=player.Name   .. "_ESP" ;boxAdorn.Adornee=hrp;boxAdorn.AlwaysOnTop=true;boxAdorn.ZIndex=10;boxAdorn.Size=Vector3.new(4,5,2);boxAdorn.Color3=Color3.fromRGB(0,255,0);boxAdorn.Transparency=0.5;boxAdorn.Parent=espFolder;local billboard=Instance.new("BillboardGui");billboard.Name=player.Name   .. "_Name" ;billboard.Adornee=hrp;billboard.Size=UDim2.new(0,100,0,30);billboard.StudsOffset=Vector3.new(0,3,0);billboard.AlwaysOnTop=true;billboard.Parent=espFolder;local nameLabel=Instance.new("TextLabel",billboard);nameLabel.Size=UDim2.new(1,0,1,0);nameLabel.BackgroundTransparency=1;nameLabel.Text=player.Name;nameLabel.TextColor3=Color3.fromRGB(255,255,0);nameLabel.TextStrokeTransparency=0;nameLabel.TextStrokeColor3=Color3.new(0,0,0);nameLabel.Font=Enum.Font.GothamBold;nameLabel.TextSize=14;nameLabel.TextScaled=true;end);end local function removerESP(player) pcall(function() local box=espFolder:FindFirstChild(player.Name   .. "_ESP" );if box then box:Destroy();end local name=espFolder:FindFirstChild(player.Name   .. "_Name" );if name then name:Destroy();end end);end RunService.Heartbeat:Connect(function() for _,player in Players:GetPlayers() do if ((player~=LocalPlayer) and player.Character) then pcall(function() local hrp=player.Character:FindFirstChild("HumanoidRootPart");for _,part in player.Character:GetDescendants() do if part:IsA("BasePart") then part.LocalTransparencyModifier= -1;end end if hrp then local boxEsp=espFolder:FindFirstChild(player.Name   .. "_ESP" );if  not boxEsp then aplicarESP(player);end end end);end end end);local playerSelecionado=nil;local function setupPlayer(player) if (player==LocalPlayer) then return;end local function onChar(char) task.wait(0.3);aplicarESP(player);end player.CharacterAdded:Connect(onChar);if player.Character then task.spawn(function() task.wait(0.3);aplicarESP(player);end);end end local function atualizarBotoes() for _,btn in scroll:GetChildren() do if btn:IsA("TextButton") then local stroke=btn:FindFirstChildOfClass("UIStroke");if (playerSelecionado and (btn.Name==playerSelecionado.Name)) then btn.BackgroundColor3=Color3.fromRGB(0,80,0);if stroke then stroke.Color=Color3.fromRGB(0,255,0);stroke.Transparency=0;end else btn.BackgroundColor3=Color3.fromRGB(15,15,15);if stroke then stroke.Color=Color3.fromRGB(0,255,255);stroke.Transparency=0.5;end end end end if playerSelecionado then title.Text="Selecionado: "   .. playerSelecionado.Name   .. " [Q]" ;else title.Text="Vands gostozo ðŸ‘„ðŸ¥’";end end local function onPlayerAdded(player) if (player==LocalPlayer) then return;end local btn=Instance.new("TextButton",scroll);btn.Name=player.Name;btn.Size=UDim2.new(1,0,0,32);btn.BackgroundColor3=Color3.fromRGB(15,15,15);btn.Text=player.Name;btn.TextColor3=Color3.new(1,1,1);btn.Font=Enum.Font.GothamBold;btn.TextSize=15;Instance.new("UICorner",btn).CornerRadius=UDim.new(0,8);local stroke=Instance.new("UIStroke",btn);stroke.Color=Color3.fromRGB(0,255,255);stroke.Transparency=0.5;btn.MouseEnter:Connect(function() if  not (playerSelecionado and (btn.Name==playerSelecionado.Name)) then stroke.Transparency=0;end end);btn.MouseLeave:Connect(function() if  not (playerSelecionado and (btn.Name==playerSelecionado.Name)) then stroke.Transparency=0.5;end end);btn.MouseButton1Click:Connect(function() playerSelecionado=player;atualizarBotoes();end);task.wait(0.1);scroll.CanvasSize=UDim2.new(0,0,0,layout.AbsoluteContentSize.Y);setupPlayer(player);end local function onPlayerRemoving(player) if (playerSelecionado and (playerSelecionado==player)) then playerSelecionado=nil;atualizarBotoes();end for _,btn in scroll:GetChildren() do if (btn.Name==player.Name) then btn:Destroy();break;end end task.wait(0.1);scroll.CanvasSize=UDim2.new(0,0,0,layout.AbsoluteContentSize.Y);removerESP(player);end local function executarComandosNoSelecionado() if  not playerSelecionado then return;end for _,cmd in ipairs(comandos) do pcall(function() TextChatService.TextChannels.RBXGeneral:SendAsync(";"   .. cmd   .. " "   .. playerSelecionado.Name );end);task.wait(0.15);end end for _,player in Players:GetPlayers() do onPlayerAdded(player);end Players.PlayerAdded:Connect(onPlayerAdded);Players.PlayerRemoving:Connect(onPlayerRemoving);local speedBtn=Instance.new("TextButton",frame);speedBtn.Size=UDim2.new(1, -20,0,40);speedBtn.Position=UDim2.new(0,10,1, -55);speedBtn.BackgroundColor3=Color3.fromRGB(25,25,25);speedBtn.Text="[F] Speed (5s)";speedBtn.Font=Enum.Font.GothamBlack;speedBtn.TextSize=16;speedBtn.TextColor3=Color3.new(1,1,1);Instance.new("UICorner",speedBtn).CornerRadius=UDim.new(0,10);local speedAtivo=false;local speedValue=28;local duracao=5;RunService.Heartbeat:Connect(function() if speedAtivo then pcall(function() local char=LocalPlayer.Character;if char then local humanoid=char:FindFirstChild("Humanoid");if humanoid then humanoid.WalkSpeed=speedValue;end end end);end end);local function ativarSpeed() if speedAtivo then return;end speedAtivo=true;speedBtn.BackgroundColor3=Color3.fromRGB(0,80,0);for i=duracao,1, -1 do speedBtn.Text="[F] Speed ("   .. i   .. "s)" ;task.wait(1);end speedAtivo=false;speedBtn.Text="[F] Speed (5s)";speedBtn.BackgroundColor3=Color3.fromRGB(25,25,25);pcall(function() local char=LocalPlayer.Character;if (char and char:FindFirstChild("Humanoid")) then char.Humanoid.WalkSpeed=16;end end);end speedBtn.MouseButton1Click:Connect(ativarSpeed);UserInputService.InputBegan:Connect(function(input,gameProcessed) if gameProcessed then return;end if (input.KeyCode==Enum.KeyCode.F) then ativarSpeed();elseif (input.KeyCode==Enum.KeyCode.Q) then executarComandosNoSelecionado();end end);local dragging=false;local dragStart,startPos;title.InputBegan:Connect(function(input) if (input.UserInputType==Enum.UserInputType.MouseButton1) then dragging=true;dragStart=input.Position;startPos=frame.Position;end end);title.InputEnded:Connect(function(input) if (input.UserInputType==Enum.UserInputType.MouseButton1) then dragging=false;end end);UserInputService.InputChanged:Connect(function(input) if (dragging and (input.UserInputType==Enum.UserInputType.MouseMovement)) then local delta=input.Position-dragStart ;frame.Position=UDim2.new(startPos.X.Scale,startPos.X.Offset + delta.X ,startPos.Y.Scale,startPos.Y.Offset + delta.Y );end end);UserInputService.InputBegan:Connect(function(input,gp) if gp then return;end if (input.KeyCode==Enum.KeyCode.R) then pcall(function() LocalPlayer:Kick("SE E LOUCO NÃ‚O COMPEMSA.");end);end end); end
